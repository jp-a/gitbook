{
    "gitbook": "2.x.x",
    "plugins": [
        "edit-link",
        "prism",
        "-highlight",
        "github",
        "anker-enable",
        "-image-captions",
        "svg"
        
    ],
    "structure": {
        "summary": "docs/SUMMARY.md"
    },
    "pluginsConfig": {
      "edit-link": {
                "base": "https://github.com/jp-a/gitbook/edit/master/",
                "label": "Edit This Page"
            },
      "github": {
        "url": "https://github.com/reactjs/redux/"
      }
    }
}